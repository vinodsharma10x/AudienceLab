{
  "name": "audiencelab",
  "version": "1.0.0",
  "description": "Marketing Research & Content Generation Platform",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && source venv/bin/activate && python start.py",
    "frontend": "cd frontend && npm start",
    "install-all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "fastapi",
    "supabase",
    "authentication",
    "dashboard"
  ],
  "author": "AudienceLab Team",
  "license": "MIT"
}
